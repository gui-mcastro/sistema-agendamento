{
  "name": "gep/cerebro",
  "description": "This system is used by Sebrae Company",
  "homepage": "http://intranetteste.df.sebrae.com.br/",
  "type": "project",
  "require": {
    "php": ">=8.3.0",
    "yiisoft/yii2": "~2",
    "yiisoft/yii2-bootstrap5": "~2.0.4",
    "yiisoft/yii2-swiftmailer": "~2.1.3",
    "nterms/yii2-pagesize-widget": "~2.0.1",
    "kartik-v/yii2-widget-select2": "~2.2.5",
    "kartik-v/yii2-widget-growl": "~1.1.2",
    "kartik-v/yii2-widget-typeahead": "~1.0.4",
    "kartik-v/yii2-widget-depdrop": "~1.0.6",
    "kartik-v/yii2-widget-timepicker": "~1.0.5",
    "kartik-v/yii2-widget-datetimepicker": "~1.5.1",
    "kartik-v/dependent-dropdown": "~1.4.9",
    "kartik-v/yii2-widget-datepicker": "~1.4.8",
    "kartik-v/yii2-widget-activeform": "~1.6.4",
    "yiibr/yii2-br-validator": "~1.1.1",
    "phpmailer/phpmailer": "~6.9.3",
    "edvlerblog/yii2-adldap-module": "~6.0.0",
    "kartik-v/bootstrap-fileinput": "~5.5.4",
    "kartik-v/yii2-widget-colorinput": "~1.0.6",
    "kartik-v/yii2-grid": "~3.5.3",
    "nesbot/carbon": "~3.8.4",
    "kartik-v/yii2-widgets": "~3.4.1",
    "mike42/escpos-php": "~4.0",
    "kartik-v/yii2-money": "~1.2.3",
    "mashape/unirest-php": "~3.0.4",
    "setasign/fpdi-tcpdf": "~2.3.0",
    "phpseclib/phpseclib": "~3.0.43",
    "caio-brendo/yii2-dynamicgridform": "~1",
    "guzzlehttp/guzzle": "~7.9.2",
    "kartik-v/yii2-export": "~1.4.3",
    "caio-brendo/yii2-dynamicform": "~2.0",
    "kartik-v/yii2-mpdf": "~1.0.6",
    "2amigos/qrcode-library": "~3.1.0",
    "2amigos/yii2-chartjs-widget": "~3.0.1",
    "setasign/fpdf": "~1.8.6",
    "setasign/fpdi": "~2.6.2",
    "picqer/php-barcode-generator": "~3.2.0",
    "rmrevin/yii2-fontawesome": "~3.5"
  },
  "minimum-stability": "stable",
  "replace": {
    "npm-asset/bootstrap": ">=5.1"
  },
  "require-dev": {
    "yiisoft/yii2-debug": "^2.1.25",
    "yiisoft/yii2-gii": "^2.2.6",
    "yiisoft/yii2-faker": "^2.0.5",
    "codeception/verify": "^3.2.0",
    "codeception/specify": "^2.0.0",
    "php-parallel-lint/php-parallel-lint": "^1.4.0",
    "php-parallel-lint/php-console-highlighter": "^1.0.0"
  },
  "config": {
    "fxp-asset": {
      "installer-paths": {
        "npm-asset-library": "vendor/npm",
        "bower-asset-library": "vendor/bower"
      }
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  },
  "scripts": {
    "post-install-cmd": [
      "yii\\composer\\Installer::postInstall"
    ],
    "post-create-project-cmd": [
      "yii\\composer\\Installer::postCreateProject",
      "yii\\composer\\Installer::postInstall"
    ]
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    },
    "yii\\composer\\Installer::postCreateProject": {
      "setPermission": [
        {
          "runtime": "0777",
          "web/assets": "0777",
          "yii": "0755"
        }
      ]
    },
    "yii\\composer\\Installer::postInstall": {
      "generateCookieValidationKey": [
        "config/web.php"
      ]
    },
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "git",
      "url": "https://github.com/caio-brendo/yii2-dynamicform.git"
    }
  ]
}
